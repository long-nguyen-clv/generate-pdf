<html>
  <head>
    <meta charset='utf-8' />
  </head>
  <body>
    <img src='logo.png' alt='logo' class='logo' />

    <h1 class='title'>Reefer IoT Sensor Report</h1>
    <div class='date'>
      Download Date:
      {{date}}
    </div>

    <div class='info'>
      {{#each info as |value|}}
        <div> {{value.label}}</div>
        <div> {{value.value}}</div>
      {{/each}}
    </div>
    <table>
      <thead>
        <tr>
          {{#each nameOfColumn as |value|}}
            <th> {{value}}</th>
          {{/each}}
        </tr>
      </thead>

      {{#each data as |row|}}
        <tr>
          {{#each row as |cell|}}
            <td>{{cell}}</td>
          {{/each}}
        </tr>
      {{/each}}

    </table>
  </body>
</html>